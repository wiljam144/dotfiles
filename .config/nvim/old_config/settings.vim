colorscheme spaceduck
syntax enable
set termguicolors
set background=dark

set virtualedit=onemore

set autoindent
set expandtab
set shiftwidth=4
set smarttab
set tabstop=4
filetype plugin indent on

set number relativenumber
set cursorline
set encoding=UTF-8

set laststatus=2
set noshowmode

set clipboard+=unnamedplus
